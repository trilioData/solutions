SELECT del_table.vm_network_resource_snap_id FROM vm_network_resource_snaps AS del_table JOIN (SELECT main.vm_network_resource_snap_id FROM vm_network_resource_snaps AS main INNER JOIN snapshot_vm_resources AS sub1 INNER JOIN snapshots AS sub2 WHERE main.vm_network_resource_snap_id = sub1.id AND sub1.snapshot_id = sub2.id AND sub2.deleted = 1 LIMIT #LIMIT#) del_select ON del_select.vm_network_resource_snap_id = del_table.vm_network_resource_snap_id
