SELECT del_table.id FROM vm_disk_resource_snap_metadata AS del_table JOIN (SELECT main.id FROM vm_disk_resource_snap_metadata AS main INNER JOIN vm_disk_resource_snaps AS sub1 INNER JOIN snapshot_vm_resources AS sub2 INNER JOIN snapshots AS sub3 WHERE main.vm_disk_resource_snap_id = sub1.id AND sub1.snapshot_vm_resource_id = sub2.id AND sub2.snapshot_id = sub3.id AND sub3.deleted = 1 LIMIT #LIMIT#) del_select ON del_table.id = del_select.id
