SELECT del_table.id FROM vm_network_resource_snap_metadata AS del_table JOIN (SELECT main.id FROM vm_network_resource_snap_metadata AS main INNER JOIN vm_network_resource_snaps AS sub1 INNER JOIN snapshot_vm_resources AS sub2 INNER JOIN snapshots AS sub3 WHERE main.vm_network_resource_snap_id = sub1.vm_network_resource_snap_id AND sub1.vm_network_resource_snap_id = sub2.id AND sub2.snapshot_id = sub3.id AND sub3.deleted = 1 LIMIT #LIMIT#) del_select ON del_select.id = del_table.id
