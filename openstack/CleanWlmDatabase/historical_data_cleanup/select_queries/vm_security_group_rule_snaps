SELECT del_table.id FROM vm_security_group_rule_snaps AS del_table JOIN (SELECT main.id FROM vm_security_group_rule_snaps AS main INNER JOIN snapshot_vm_resources AS sub1 INNER JOIN snapshots AS sub2 WHERE main.vm_security_group_snap_id = sub1.id AND sub1.snapshot_id = sub2.id AND sub2.deleted = 1 LIMIT #LIMIT#) del_select ON del_select.id = del_table.id
